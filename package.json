    "@storyblok/react": "^2.1.4",
    "@vercel/node": "^2.14.5",
    "clsx": "^1.2.1",
    "isbot": "^3.6.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.9.0",
    "storyblok-rich-text-react-renderer": "^2.8.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.16.1",
